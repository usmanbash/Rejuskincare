      <div class="video-slider"
           data-desktop="{{ block.settings.slides_desktop }}"
           data-tablet="{{ block.settings.slides_tablet }}"
           data-mobile="{{ block.settings.slides_mobile }}">

        {% for i in (1..8) %}
          {% assign use_video_key = 'use_video_' | append: i %}
          {% assign video_url_key = 'video_url_' | append: i %}
          {% assign use_video = block.settings[use_video_key] %}
          {% assign video_url = block.settings[video_url_key] %}
