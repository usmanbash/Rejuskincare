                              {%- if block.settings[badge_key] != blank -%}
                  {%- assign badge_value = block.settings[badge_key] | strip -%}
                  {%- assign badge_price = badge_value | split: ' ' | first -%}
                  {%- assign badge_rest = badge_value | remove_first: badge_price | strip -%}
