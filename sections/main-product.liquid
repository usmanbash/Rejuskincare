                          {%- assign badge_key = 'gift_' | append: i | append: '_badge' -%}
                          {%- assign image_key = 'gift_' | append: i | append: '_image' -%}
                          {%- assign subtitle_key = 'gift_' | append: i | append: '_subtitle' -%}
                          {%- if block.settings[title_key] != blank or block.settings[badge_key] != blank or block.settings[image_key] != blank or block.settings[subtitle_key] != blank -%}
