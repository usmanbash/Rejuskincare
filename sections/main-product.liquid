      if (!priceCurrent || !priceCompare) return;
      
      const currentPriceText = priceCurrent.textContent.trim();
      const comparePriceText = priceCompare.textContent.trim();
      
      // Update all price display areas (mobile and desktop)
      const priceContainers = document.querySelectorAll('#price-{{ section.id }}');
      priceContainers.forEach(priceContainer => {
        const priceRegular = priceContainer.querySelector('.price__regular .price-item--regular');
        const priceSale = priceContainer.querySelector('.price__sale');
        const priceSaleCurrent = priceContainer.querySelector('.price__sale .price-item--sale');
        const priceSaleCompare = priceContainer.querySelector('.price__sale .price-item--regular');
        
        // Show sale price if there's a discount
        if (priceCurrent.dataset.price !== priceCompare.dataset.price) {
          priceContainer.classList.add('price--on-sale');
          priceContainer.classList.remove('price--no-compare');
          
          // Update sale price elements
          if (priceSaleCompare) {
            priceSaleCompare.innerHTML = comparePriceText;
          }
          if (priceSaleCurrent) {
            priceSaleCurrent.innerHTML = currentPriceText;
          }
        } else {
          // No discount, show regular price
          priceContainer.classList.remove('price--on-sale');
          
          if (priceRegular) {
            priceRegular.innerHTML = currentPriceText;
          }
        }
      });
      
      // Update ATC button
      if (submitButton) {
        const submitButtonText = submitButton.querySelector('span');
        if (submitButtonText) {
          if (priceCurrent.dataset.price !== priceCompare.dataset.price) {
            // Show price with strikethrough
            submitButtonText.innerHTML = `${originalButtonText} - <s>${comparePriceText}</s> ${currentPriceText}`;
          } else {
            // Show price without strikethrough
            submitButtonText.innerHTML = `${originalButtonText} - ${currentPriceText}`;
          }
        }
      }
    };
    
    // Set initial quantity and price
    const checkedRadio = bundleSelector.querySelector('.bundle-option__radio:checked');
    if (checkedRadio) {
      const qtyInput = getQuantityInput();
      if (qtyInput) qtyInput.value = checkedRadio.dataset.quantity;
      
      const bundleOption = checkedRadio.closest('.bundle-option');
      updatePriceDisplay(bundleOption);
    }
    
    // Handle bundle selection
    bundleSelector.querySelectorAll('.bundle-option__radio').forEach(radio => {
      radio.addEventListener('change', (e) => {
        bundleSelector.querySelectorAll('.bundle-option').forEach(opt => 
          opt.classList.remove('bundle-option--selected')
        );
        e.target.closest('.bundle-option').classList.add('bundle-option--selected');
        
        const qtyInput = getQuantityInput();
        if (qtyInput) {
          qtyInput.value = e.target.dataset.quantity;
          qtyInput.dispatchEvent(new Event('change', { bubbles: true }));
        }
        
        // Update price display
        const bundleOption = e.target.closest('.bundle-option');
        updatePriceDisplay(bundleOption);
        
        // Update price badge with save text
        const saveTextElement = bundleOption.querySelector('.bundle-option__save');
        const priceBadge = document.querySelector('.price__badge-sale');
        if (saveTextElement && priceBadge) {
          priceBadge.textContent = saveTextElement.textContent.trim();
        }
      });
    });
    
    // Set initial price badge text if bundle is pre-selected
    const initialCheckedRadio = bundleSelector.querySelector('.bundle-option__radio:checked');
    if (initialCheckedRadio) {
      const initialSaveText = initialCheckedRadio.closest('.bundle-option').querySelector('.bundle-option__save');
      const priceBadge = document.querySelector('.price__badge-sale');
      if (initialSaveText && priceBadge) {
        priceBadge.textContent = initialSaveText.textContent.trim();
      }
    }
  });
</script>

